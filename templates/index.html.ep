<!DOCTYPE html>

<html>
<head>
	<link rel="stylesheet" href="/site.css">
</head>

<script src="/scheduler.js"> </script>

<script>

function resetToIndex()
{
	let page = window.location.href + "";
	let match = page.match( /index|\/$/);
	if ( match == null)
	{
		window.location = "<%= url_for('/')->to_abs %>";
	}
}


window.addEventListener( "pageshow", resetToIndex);

</script>

<body>
	<div id="navbar">
	</div>

	<h1>Scheduler</h1>


</body>

</html>

